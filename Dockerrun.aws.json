{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "youngsingtan/multi-client:latest",
			"hostname": "client",
			"essential": false,
			"memory": 128
		},
		{
			"name": "server",
			"image": "youngsingtan/multi-server:latest",
			"hostname": "api",
			"essential": false,
			"memory": 128
		},
		{
			"name": "worker",
			"image": "youngsingtan/multi-worker:latest",
			"hostname": "worker",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "youngsingtan/mutil-nginx:latest",
			"hostname": "nginx",
			"essential": true,
			"memory": 128,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": [
				"client",
				"server"
			]
		}
	]
}